@using TatilSeyahatSitesi.Models.Siniflar
@model List<Blog>

<div class="padding1">
    <h4>Farklı Deneyimlerim</h4>
    <div class="box-left">
        <h3>En Beğendiklerim</h3>
        @foreach (var item in Model)
        {

            <div class="bot-img">
                <a href="/Blog/BlogDetay/@item.ID/"> <img src="@item.BlogImage" alt=""  width="120px" height="74px" /></a>
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0,70) ... </p>
            </div>
            <div class="clear"></div>
        }

    </div>
    @Html.Action("Partial5", "Default")
</div> 
